r\n    <div class=\"editUser-box\">\r\n        <h1> 개인정보 변경 </h1>\r\n\r\n        <div class=\"text-box\">\r\n          <label for=\"nickname\">닉네임 : </label>\r\n          <input \r\n              v-model=\"myInfo.nickname\" \r\n              id=\"nickname\"\r\n              name=\"nickname\"\r\n              required>\r\n        </div>\r\n\r\n        <div class=\"text-box\">\r\n          <label for=\"lastName\">성 : </label>\r\n          <input \r\n              v-model=\"myInfo.lastName\" \r\n              id=\"lastName\"\r\n              name=\"lastName\"\r\n              required>\r\n        </div>\r\n\r\n        <div class=\"text-box\">\r\n          <label for=\"firstName\">이름 : </label>\r\n          <input \r\n              v-model=\"myInfo.firstName\" \r\n              id=\"firstName\"\r\n              name=\"firstName\"\r\n              required>\r\n        </div>\r\n\r\n        <div class=\"text-box\">\r\n          <label>성별 : </label>\r\n          <input\r\n              type=\"radio\"\r\n              id=\"male\"\r\n              value=\"남자\"\r\n              v-model=\"myInfo.avatar\"> Male\r\n          <input\r\n              type=\"radio\"\r\n              id=\"female\"\r\n              value=\"여자\"\r\n              v-model=\"myInfo.avatar\"> Female  \r\n        </div>\r\n\r\n        <div class=\"text-box\">\r\n          <label for=\"age\">나이 : </label>\r\n          <input \r\n              v-model.number=\"myInfo.age\" \r\n              id=\"age\"\r\n              name=\"age\" \r\n              maxlength=\"2\"\r\n              required>       \r\n        </div>\r\n\r\n        <div class=\"text-box\">\r\n          <label for=\"address\">주소 : </label>\r\n          <input \r\n              v-model=\"myInfo.address\" \r\n              id=\"address\"\r\n              name=\"address\"\r\n              required>\r\n        </div>\r\n\r\n        <div class=\"text-box\">\r\n          <label>핸드폰 : </label>\r\n          <input\r\n                v-model=\"myInfo.phone_first\" \r\n                name=\"phone_first\"\r\n                maxlength=\"3\"\r\n                class=\"phone\" required> - <input \r\n                                                v-model=\"myInfo.phone_middle\" \r\n                                                name=\"phone_middle\" \r\n                                                maxlength=\"4\"\r\n                                                class=\"phone\" required> - <input\r\n                                                                                v-model=\"myInfo.phone_last\" \r\n                                                                                name=\"phone_last\"\r\n                                                                                maxlength=\"4\"\r\n                                                                                class=\"phone\" required>\r\n        </div>\r\n\r\n        <div class=\"submit\">\r\n              <button class=\"btn\" @click=\"editUser\">수정</button>\r\n        </div>\r\n    </div>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data: function () {\r\n    return {\r\n      myInfo: {}\r\n    }\r\n  },\r\n  methods: {\r\n    editUser() {\r\n      this.$http.put('/api/login/' + this.$route.params.id + '/myPage/edit', {myInfo: this.myInfo})\r\n      .then((response) => {\r\n        console.log('response');\r\n        console.log(response.data);\r\n        if (response.data.result == 'fail') {\r\n          alert('Error, please, try again');\r\n        }\r\n        else {\r\n          let nickname = response.data.nickname;\r\n          localStorage.setItem(\"nickname\", nickname);\r\n          this.$store.state.nickname = nickname;\r\n          alert('Success');\r\n          this.$router.push('/' + this.$route.params.id + '/myPage' );\r\n        }\r\n      })\r\n      .catch(function (error) {\r\n        alert('frontend error');\r\n        console.log(error);\r\n      })\r\n    }\r\n  },\r\n\r\n  created
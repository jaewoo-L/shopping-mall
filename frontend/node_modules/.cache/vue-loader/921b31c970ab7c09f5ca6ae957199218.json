     alert('제품 등록 오류');\r\n              }\r\n              this.$router.push('/products');\r\n            })\r\n            .catch(function (error) {\r\n                  console.log(error);\r\n             })\r\n      }\r\n    },\r\n\r\n  validations: {\r\n    name: {\r\n      required\r\n    },\r\n    price: {\r\n      required\r\n    },\r\n    kinds: {\r\n      required\r\n    },\r\n    brand: {\r\n      required\r\n    },\r\n    SItems: {\r\n      required,\r\n      numeric\r\n    },\r\n    MItems: {\r\n      required,\r\n      numeric\r\n    },\r\n    LItems: {\r\n      required,\r\n      numeric\r\n    },\r\n    XLItems: {\r\n      required,\r\n      numeric\r\n    },\r\n    avatar: {\r\n      required\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style src=\"../../../public/stylesheets/products.css\">\r\n</style>\r\n"]}]}